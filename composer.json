{
    "name": "devwizardhq/laravel-enumify",
    "description": "Auto-generate TypeScript enums and types from Laravel PHP enums with an Artisan command and Vite integration.",
    "keywords": [
        "laravel",
        "enumify",
        "typescript",
        "enums",
        "codegen"
    ],
    "homepage": "https://github.com/devwizardhq/laravel-enumify",
    "license": "MIT",
    "authors": [
        {
            "name": "IQBAL HASAN",
            "email": "iqbalhasan.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.10"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^8.0||^9.0||^10.9.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DevWizardHQ\\Enumify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevWizardHQ\\Enumify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage --min=100 --coverage-clover build/coverage.xml",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevWizardHQ\\Enumify\\EnumifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
